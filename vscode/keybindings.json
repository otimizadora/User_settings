// Place your key bindings in this file to override the defaultsauto[]
[
    {
        "key": "ctrl+alt+.",
        "command": "editor.action.copyLinesDownAction",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "ctrl+shift+alt+down",
        "command": "-editor.action.copyLinesDownAction",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "ctrl+alt+,",
        "command": "editor.action.copyLinesUpAction",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "ctrl+shift+alt+up",
        "command": "-editor.action.copyLinesUpAction",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "ctrl+shift+alt+g",
        "command": "workbench.action.toggleDevTools",
        "when": "isDevelopment"
    },
    {
        "key": "ctrl+shift+i",
        "command": "-workbench.action.toggleDevTools",
        "when": "isDevelopment"
    },
    {
        "key": "ctrl+alt+n",
        "command": "-code-runner.run"
    },
    {
        "key": "ctrl+numpad2",
        "command": "workbench.action.output.toggleOutput"
    },
    {
        "key": "ctrl+k ctrl+h",
        "command": "-workbench.action.output.toggleOutput"
    },
    {
        "key": "numpad_separator",
        "command": "workbench.output.action.clearOutput"
    },
    {
        "key": "alt+numpad_multiply",
        "command": "git.showOutput"
    },
    {
        "key": "ctrl+numpad1",
        "command": "workbench.action.openSettingsJson"
    },
    {
        "key": "ctrl+numpad3",
        "command": "workbench.view.explorer"
    },
    {
        "key": "ctrl+shift+e",
        "command": "-workbench.view.explorer"
    },
    {
        "key": "numpad8 ctrl+alt+",
        "command": "workbench.action.quickOpenNavigatePreviousInFilePicker",
        "when": "inFilesPicker && inQuickOpen"
    },
    {
        "key": "ctrl+shift+e",
        "command": "-workbench.action.quickOpenNavigatePreviousInFilePicker",
        "when": "inFilesPicker && inQuickOpen"
    },
    {
        "key": "ctrl+numpad_multiply",
        "command": "workbench.files.action.focusOpenEditorsView",
        "when": "workbench.explorer.openEditorsView.active"
    },
    {
        "key": "ctrl+k e",
        "command": "-workbench.files.action.focusOpenEditorsView",
        "when": "workbench.explorer.openEditorsView.active"
    },
    {
        "key": "alt+numpad6",
        "command": "workbench.view.scm"
    },
    {
        "key": "ctrl+shift+g",
        "command": "-workbench.view.scm"
    },
    {
        "key": "ctrl+shift+p",
        "command": "workbench.action.showCommands"
    },
    {
        "key": "ctrl+shift+p",
        "command": "-workbench.action.showCommands"
    },
    {
        "key": "alt+numpad1",
        "command": "workbench.action.terminal.toggleTerminal"
    },
    {
        "key": "ctrl+shift+[BracketLeft]",
        "command": "-workbench.action.terminal.toggleTerminal"
    },
    {
        "key": "alt+numpad3",
        "command": "workbench.action.toggleSidebarVisibility"
    },
    {
        "key": "ctrl+b",
        "command": "-workbench.action.toggleSidebarVisibility"
    },
    {
        "key": "alt+numpad9",
        "command": "workbench.action.toggleActivityBarVisibility"
    },
    {
        "key": "alt+numpad7",
        "command": "editor.action.toggleMinimap"
    },
    {
        "key": "alt+numpad_separator",
        "command": "workbench.actions.view.problems"
    },
    {
        "key": "ctrl+shift+m",
        "command": "-workbench.actions.view.problems"
    },
    {
        "key": "alt+numpad2",
        "command": "workbench.action.toggleMaximizedPanel"
    },
    {
        "key": "ctrl+numpad9",
        "command": "workbench.explorer.fileView.focus"
    },
    {
        "key": "ctrl+shift+c",
        "command": "-workbench.action.terminal.openNativeConsole",
        "when": "!terminalFocus"
    },
    {
        "key": "ctrl+numpad2 alt+numpad2",
        "command": "workbench.action.toggleStatusbarVisibility"
    },
    {
        "key": "ctrl+k ctrl+b",
        "command": "-workbench.action.toggleSidebarVisibility"
    },
    {
        "key": "ctrl+shift+p",
        "command": "-workbench.action.quickOpenNavigatePreviousInFilePicker",
        "when": "inFilesPicker && inQuickOpen"
    },
    {
        "key": "ctrl+shift+numpad5 ctrl+shift+right",
        "command": "workbench.action.openGlobalKeybindingsFile"
    },
    {
        "key": "ctrl+shift+down ctrl+shift+numpad5",
        "command": "workbench.action.openDefaultKeybindingsFile"
    },
    {
        "key": "ctrl+numpad_multiply ctrl+numpad_multiply",
        "command": "eslint.showOutputChannel"
    },
    {
        "key": "ctrl+f9",
        "command": "editor.action.openLink"
    },
    {
        "key": "ctrl+numpad5",
        "command": "bookmarks.toggle",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+alt+k",
        "command": "-bookmarks.toggle",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+alt+k",
        "command": "-code-runner.runCustomCommand"
    },
    {
        "key": "alt+numpad0",
        "command": "code-runner.run"
    },
    {
        "key": "ctrl+numpad0",
        "command": "-workbench.action.zoomReset"
    },
    {
        "key": "ctrl+alt+l",
        "command": "-bookmarks.jumpToNext",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+alt+j",
        "command": "-bookmarks.jumpToPrevious",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+numpad0",
        "command": "bookmarks.clear"
    },
    {
        "key": "ctrl+numpad6",
        "command": "bookmarks.jumpToNext"
    },
    {
        "key": "ctrl+numpad4",
        "command": "bookmarks.jumpToPrevious"
    },
    {
        "key": "ctrl+alt+numpad0",
        "command": "bookmarks.clearFromAllFiles"
    }
]
